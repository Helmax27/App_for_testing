<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="borderPane" prefHeight="900.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NewsampleController">
   <top>
      <HBox prefHeight="176.0" prefWidth="1920.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar fx:id="menuBar" prefHeight="25.0" prefWidth="200.0">
              <menus>
                <Menu fx:id="testItem" mnemonicParsing="false" onAction="#onClick" text="Test">
                  <items>
                    <MenuItem fx:id="exitItem" mnemonicParsing="false" onAction="#exitAction" text="Exit" />
                        <MenuItem fx:id="startItem" mnemonicParsing="false" onAction="#startAction" text="Start" />
                  </items>
                </Menu>
                <Menu fx:id="viewItem" mnemonicParsing="false" onAction="#onClick" text="View">
                  <items>
                    <MenuItem fx:id="showLogItem" mnemonicParsing="false" onAction="#showLogAction" text="Show All logs" />
                        <MenuItem fx:id="showDescriptionItem" mnemonicParsing="false" onAction="#showDescriptionAction" text="Show description" />
                  </items>
                </Menu>
                <Menu fx:id="settingItem" mnemonicParsing="false" onAction="#onClick" text="Setting">
                  <items>
                    <MenuItem fx:id="profilesItem" mnemonicParsing="false" onAction="#profileAction" text="Profiles" />
                        <MenuItem fx:id="parametersItem" mnemonicParsing="false" onAction="#parametersAction" text="Parameters" />
                        <MenuItem fx:id="loopSettingItem" mnemonicParsing="false" onAction="#loopSettingaction" text="Loop Settings" />
                  </items>
                </Menu>
                  <Menu fx:id="helpItem" mnemonicParsing="false" onAction="#helpAction" text="Help">
                     <items>
                        <MenuItem fx:id="aboutItem" mnemonicParsing="false" onAction="#aboutAction" text="About" />
                     </items>
                  </Menu>
              </menus>
            </MenuBar>
            <Button fx:id="startButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#startAction" prefHeight="86.0" prefWidth="86.0" style="-fx-background-radius: 8px;">
               <HBox.margin>
                  <Insets top="90.0" />
               </HBox.margin>
               <graphic>
                  <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                     <children>
                        <ImageView fitHeight="86.0" fitWidth="86.0" pickOnBounds="true" preserveRatio="true">                       <image>
                              <Image url="@assets/Start.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </graphic>
            </Button>
             <Button fx:id="stopButton" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onClick" prefHeight="86.0" prefWidth="86.0">
                 <HBox.margin>
                     <Insets top="90.0" />
                 </HBox.margin>
                 <graphic>
                     <AnchorPane fx:id="anPanStop" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                         <children>
                             <ImageView fx:id="imStop" fitHeight="86.0" fitWidth="86.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                     <Image url="@assets/Stop_button.png" />
                                 </image>
                             </ImageView>
                         </children>
                     </AnchorPane>
                 </graphic>
             </Button>
            <Button fx:id="viewButton" layoutX="296.0" layoutY="10.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" style="-fx-background-radius: 8px;" text="Button">
               <HBox.margin>
                  <Insets top="90.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="updateButton" layoutX="382.0" layoutY="10.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="Button">
               <HBox.margin>
                  <Insets top="90.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <left>
      <VBox prefHeight="864.0" prefWidth="254.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="profileListLable" contentDisplay="TOP" text="Profile List">
               <font>
                  <Font name="Calibri" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="80.0" top="25.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="profile1Button" mnemonicParsing="false" prefHeight="86.0" prefWidth="166.0" style="-fx-border-radius: 8px;" text="Profile1">
               <VBox.margin>
                  <Insets left="44.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri Bold" size="26.0" />
               </font></Button>
            <Button fx:id="profile2Button" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="166.0" style="-fx-border-radius: 8px;" text="Profile2">
               <VBox.margin>
                  <Insets left="44.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri Bold" size="26.0" />
               </font></Button>
            <Button fx:id="profile3Button" layoutX="10.0" layoutY="35.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="166.0" style="-fx-border-radius: 8px;" text="Profile3" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="44.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri Bold" size="26.0" />
               </font></Button>
            <Button fx:id="profile4Button" layoutX="10.0" layoutY="61.0" mnemonicParsing="false" prefHeight="86.0" prefWidth="166.0" style="-fx-border-radius: 8px;" text="Profile4" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="44.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="Calibri Bold" size="26.0" />
               </font></Button>
         </children>
      </VBox>
   </left>
   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="75.0" text="NAME" />
          <TableColumn prefWidth="75.0" text="DURATION" />
            <TableColumn prefWidth="75.0" text="PASS" />
            <TableColumn prefWidth="75.0" text="FAIL" />
            <TableColumn prefWidth="75.0" text="PARAMETERS" />
        </columns>
         <BorderPane.margin>
            <Insets top="100.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <right>
      <VBox prefHeight="775.0" prefWidth="690.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
   </right>
</BorderPane>
